@import "./style/iconfont.wxss";
/*在微信小程序中是不支持通配符*的*/
page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
/*
 主题颜色 通过变量来实现
1  less中 存在 变量这个知识
2 原生的css和wxss也是支持变量的
*/
page{
  /*定义主题颜色*/
--themeColor:#eb4450;
font-size: 28rpx;
}
image{
  width: 100%;
}
