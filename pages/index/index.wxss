.swp_index swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_cates {
  display: flex;
}
.index_cates navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cates navigator image {
  width: 100%;
}
.index_floor .floor_group .floor_titles {
  padding: 10rpx 0;
}
.index_floor .floor_group .floor_titles image {
  width: 100%;
}
.index_floor .floor_group .floor_list {
  overflow: hidden;
}
.index_floor .floor_group .floor_list navigator {
  float: left;
  width: 33.33%;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
  /*按照比例来算
          232 / 386 = 33.33vw / height
          height = 33.33vw * 386 / 232
          这是和第一张大图片一样的比例，也是第一张大图的大小
          所以要转换成小图片的 要除以2
          小图的高：height = 33.33vw * 386 / 232 / 2

          */
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list navigator:nth-child(2),
.index_floor .floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
